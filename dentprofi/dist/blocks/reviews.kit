<script>
    let data = Array.from(document.querySelectorAll('.txt__row .txt__item')),
    step = 4,
    item = 0;

data.slice(step).forEach(e => e.style.display = 'none');
item += step;

document.querySelector('#more1').addEventListener('click', function(e){
  let tmp = data.slice(item, item + step);
  tmp.forEach(e => e.style.display = 'flex');
  item += step;
  
  if(tmp.length < 4)
    this.remove();
});
</script>

