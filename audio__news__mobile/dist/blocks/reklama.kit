	<title>Реклама</title>
</head>
<body>

<!-- @import "header.html" -->

<section class="main__container">
	<div class="container">

		

		<div class="category__title new1">Реклама</div>

		<div class="contacts__row">
			<div class="contacts__left">
				

				<div class="contacts__tel">
					<div class="contacts__tel__ico tell"><img class="svg" src="icons/call.svg"></div>
					<div class="contacts__tel__content">
						<span class="bo">Контакты</span>
						<a href="tel:+81234567890">8 (123) 456 78 90</a>
						
					</div>
				</div>

				<div class="contacts__tel">
					<div class="contacts__tel__ico mail"><img class="svg" src="icons/email.svg"></div>
					<div class="contacts__tel__content">
						<span class="bo">Иван Иванов — директор по рекламе</span>
						<a href="mailto:info@invoice.ru">info@invoice.ru</a>
						
					</div>
				</div>


			


			</div>
			<div class="contacts__right">
				<form class="order__reklama">
					<div class="order__title">ЗАЯВКА НА РЕКЛАМУ</div>
					<div class="order__txt">Оставьте ваши данные и мы свяжемся с вами</div>
					<div class="inputs__row">
						<input class="name" type="text" name="" placeholder="Ваше имя">
						<input class="email" type="text" name="" placeholder="Ваш e-mail">
						<button>Отправить</button>
					</div>
				</form>
			</div>
		</div>
		

	</div>
</section>





